{
  "classId": "idm.component.mobile.answerPaper",
  "comName": "试卷答卷",
  "className": "IanswerPaper",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
    {
      "type": "input",
      "layoutType": "inline",
      "text": "唯一标识",
      "bindKey": "ctrlId",
      "disabled": true,
      "default": "@[packageid]",
      "desc": "",
      "helpUrl": ""
    },
    {
      "type": "group",
      "text": "基本属性",
      "desc": "提供设置组件能达到展示效果的基本属性设置信息",
      "children": [
        {
          "type": "input",
          "layoutType": "inline",
          "text": "试题id",
          "bindKey": "questionId",
          "desc": "试题id",
          "default": "221011113133qXfcMBYlMF1ph0hLuxn"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "mangerId",
          "bindKey": "mangerId",
          "desc": "mangerId",
          "default": "221011112740X29h3ZepSdhluaFQxbN"
        }
      ]
    },
    {
      "type": "group",
      "text": "高级",
      "desc": "点击？查看高级设置用法指南",
      "children": [
       {
        "type": "input",
        "layoutType": "inline",
        "text": "试卷信息数据源",
        "bindKey": "dataSource",
        "default": "/ctrl/partyexma/calculateTime"
       },
       {
        "type": "input",
        "layoutType": "inline",
        "text": "试题数据源",
        "bindKey": "paperDataSource",
        "default": "/ctrl/partyexma/getAllQuestions"
       },
       {
        "type": "input",
        "layoutType": "inline",
        "text": "提交答题数据数据源",
        "bindKey": "submitPaper",
        "default": "/ctrl/partyexma/submi"
       },
       {
        "type": "input",
        "layoutType": "inline",
        "text": "提交试卷数据源",
        "bindKey": "submitPaperSend",
        "default": "/ctrl/partyexma/upsource/submitExamQQc"
       }
      ]
    }
  ]
}